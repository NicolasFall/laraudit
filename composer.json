{
    "name": "dandrea/audit",
    "description": "Package for adding basic audit capabilities to Laravel 5.5+ . It supports multiauth by providing polymorphical relations with user/auditor.",
    "license": "MIT",
    "authors": [
        {
            "name": "dandrea nicolas",
            "email": "dandrea.nicolas.ack@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Audit\\Providers\\AuditServiceProvider"
            ],
            "aliases": {
                "Audit": "Audit\\Classes\\AuditorFacade"
            }
        }
    }
}
